filters:
  and:
    - note["base"] == link("Algebra II (CB002).base")
properties:
  file.name:
    displayName: Nombre
  Descripcion:
    type: text
    displayName: Descripcion
  Fecha:
    type: datetime
    displayName: Fecha
views:
  - type: table
    name: Table View
    order:
      - file.name
      - Descripcion
      - Fecha
